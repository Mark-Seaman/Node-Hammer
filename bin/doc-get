#!/usr/bin/env python
# Get the doc from storage

from sys        import argv
from os.path    import exists,join
from os         import environ

if len(argv)<2:
    print 'usage: doc-get doc'
    exit(1)

f = join(environ['pd'],argv[1])
if not exists(f):
    print 'No doc found, %s'%f
    exit(1)

print open(f).read()


